  </servlet-mapping>
  
  
  <servlet>
  <servlet-name>asdq</servlet-name>
  <servlet-class>controller.SearchServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>asdq</servlet-name>
  <url-pattern>/search</url-pattern>
  </servlet-mapping>
  
  
   <servlet>
  <servlet-name>asdffg</servlet-name>
